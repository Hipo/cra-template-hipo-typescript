{
  "package": {
    "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "build:staging": "env-cmd -f .env.staging npm run build",
      "test": "react-scripts test",
      "eject": "react-scripts eject",
      "check-package-updates": "npx npm-check-updates",
      "prepare": "husky install"
    },
    "dependencies": {
      "classnames": "^2.3.2",
      "env-cmd": "^10.1.0",
      "react-router-dom": "^6.14.2",
      "@hipo/react-ui-toolkit": "^1.1.1"
    },
    "devDependencies": {
      "@hipo/eslint-config-base": "^4.3.0",
      "@hipo/eslint-config-react": "^2.3.0",
      "@hipo/eslint-config-typescript": "^1.2.0",
      "@hipo/stylelint-config-base": "4.0.0",
      "@types/react-router-dom": "^5.3.3",
      "eslint-config-prettier": "^8.6.0",
      "eslint-import-resolver-typescript": "^2.5.0",
      "husky": "^8.0.3",
      "lint-staged": "^13.1.2",
      "sass": "^1.63.6",
      "typescript": "^4",
      "prettier": "^2.8.4",
      "stylelint": "^15.2.0",
      "stylelint-no-unsupported-browser-features": "^6.1.0",
      "stylelint-order": "^6.0.2",
      "stylelint-scss": "^4.4.0",
      "@types/react": "*",
      "@types/react-dom": "*"
    },
    "lint-staged": {
      "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
        "prettier --config ./.prettierrc.js --write"
      ],
      "src/**/*.{js,jsx,ts,tsx}": ["eslint"],
      "src/**/*.{css,scss}": ["stylelint"]
    },
    "eslintConfig": {
      "parser": "@typescript-eslint/parser",
      "plugins": ["react", "react-hooks", "@typescript-eslint", "jsx-a11y"],
      "extends": [
        "react-app",
        "react-app/jest",
        "@hipo/eslint-config-base",
        "@hipo/eslint-config-react",
        "@hipo/eslint-config-typescript",
        "plugin:jsx-a11y/recommended",
        "prettier"
      ]
    }
  }
}
